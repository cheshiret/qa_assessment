<% if false %>
<!DOCTYPE html>
<html>
<div class="container">
  <div class="jumbotron">
    <p class="lead">


    <h1>Listing Orders</h1>

    <table class="table">
      <thead>
      <tr>
        <th>Ordid</th>
        <th>Ordnum</th>
        <th>Userid</th>
        <th>Status</th>
        <th>OrderType</th>
        <th colspan="3"></th>
      </tr>
      </thead>

      <tbody>
      <% @orders.each do |order| %>
          <tr>
            <td><%= order.ordid %></td>
            <td><%= order.ordnum %></td>
            <td><%= order.userid %></td>
            <td><%= order.status %></td>
            <td><%= order.ordertype %></td>
            <td><%= link_to 'Show', order %></td>
            <td><%= link_to 'Edit', edit_order_path(order) %></td>
            <td><%= link_to 'Destroy', order, method: :delete, data: {confirm: 'Are you sure?'} %></td>
          </tr>
      <% end %>
      </tbody>
    </table>

    <br>

    <%= link_to 'New Order', new_order_path %>
    </p>
  </div>
</div>
</html>
<% end %>
<%= render '/orders/list', action: "index" %>